{
  "version": "2.1.5",
  "private": true,
  "description": "Panda UI, A high quality Vue 3 components library",
  "author": {
    "name": "Tong Siyuan"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "workspaces": [
    "packages/*",
    "docs",
    "dev-serve"
  ],
  "scripts": {
    "test": "vitest",
    "publish": "tsx scripts/publish.ts",
    "serve": "tsx scripts/serve.ts",
    "serve:docs": "pnpm -C docs serve",
    "bootstrap": "tsx scripts/bootstrap.ts",
    "bootstrap:icons": "tsx scripts/geneIcon.ts",
    "create": "tsx scripts/create.ts",
    "lint": "eslint . --fix",
    "build": "tsx ./scripts/build.ts",
    "build:meta": "tsx ./scripts/meta.ts",
    "build:style": "gulp --require sucrase/register/ts",
    "build:docs": "pnpm -C docs build",
    "build:icons": "pnpm -C packages/icons build",
    "build:hooks": "pnpm -C packages/hooks build",
    "build:internal": "pnpm -C internal build",
    "build-all": "run-s build-common build",
    "build-common": "run-s build:internal build:hooks build:icons",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc --no-stash --allow-empty",
    "prepare": "cd .. && husky web/.husky"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@panda-ui/icons": "workspace:*",
    "dayjs": "^1.11.3",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@panda-ui/common": "workspace:*",
    "@panda-ui/components": "workspace:*",
    "@panda-ui/hooks": "workspace:*",
    "@panda-ui/icons": "workspace:*",
    "@panda-ui/internal": "workspace:*",
    "@panda-ui/styles": "workspace:*",
    "@panda-ui/tokens": "workspace:*",
    "@panda-ui/utils": "workspace:*",
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.4.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@types/eslint": "^8.37.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.14.1",
    "@types/prettier": "^2.7.2",
    "@types/prompts": "^2.4.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^9.0.0",
    "execa": "^7.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "kolorist": "^1.7.0",
    "lint-staged": "^15.2.2",
    "lucide-vue-next": "^0.395.0",
    "minimist": "^1.2.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "sucrase": "^3.32.0",
    "tsx": "^4.6.2",
    "typescript": "5.4.4",
    "unplugin-vue-define-options": "^1.3.7",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^3.5.4",
    "vitest": "^1.3.1",
    "vue": "~3.3.13",
    "vue-tsc": "^1.2.0"
  }
}
