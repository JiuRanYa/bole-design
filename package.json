{
  "private": true,
  "version": "1.13.4",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "author": {
    "name": "Tong Siyuan"
  },
  "description": "Panda UI, A high quality Vue 3 components library",
  "workspaces": [
    "packages/*",
    "docs",
    "dev-serve"
  ],
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "build-all": "pnpm -w run build-common & tsx ./scripts/build.ts",
    "build-common": "pnpm -w run build:internal && pnpm -w run build:icons",
    "build:style": "gulp --require sucrase/register/ts",
    "build:icons": "pnpm -C packages/icons build && pnpm i",
    "build:internal": "pnpm -C internal build",
    "publish": "tsx scripts/publish.ts",
    "serve": "tsx scripts/serve.ts",
    "serve:docs": "pnpm -C docs serve",
    "bootstrap": "tsx scripts/bootstrap.ts",
    "bootstrap:icons": "tsx scripts/geneIcon.ts",
    "create": "tsx scripts/create.ts"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.8",
    "@panda-ui/common": "workspace:*",
    "@panda-ui/components": "workspace:*",
    "@panda-ui/hooks": "workspace:*",
    "@panda-ui/icons": "workspace:*",
    "@panda-ui/internal": "workspace:*",
    "@panda-ui/styles": "workspace:*",
    "@panda-ui/tokens": "workspace:*",
    "@panda-ui/utils": "workspace:*",
    "dayjs": "^1.11.3",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@types/eslint": "^8.37.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.14.1",
    "@types/prettier": "^2.7.2",
    "@types/prompts": "^2.4.2",
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.4.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.38.0",
    "execa": "^7.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "kolorist": "^1.7.0",
    "minimist": "^1.2.8",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "sucrase": "^3.32.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.3",
    "unplugin-vue-define-options": "^1.3.7",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^3.5.4",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
